{
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
	"printWidth": 100,
    "arrowParens": "always",
    "endOfLine": "lf",
    "useTabs": true,
    "importOrder": ["<THIRD_PARTY_MODULES>", "^api(.*)$", "^lib/(.*)$", "^components/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderCaseInsensitive": true
}